<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :Artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :Genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name%>
  <%= f.label :Note_1%>
  <%= f.text_field :song_note_1 %>

  

  <%= f.submit %>
<% end %>
  <br />

  



